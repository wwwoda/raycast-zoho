{
  "root": true,
  "env": {
    "es2020": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint-config-woda/typescript"
  ],
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "@typescript-eslint/no-unsafe-assignment": 0
  }
}
